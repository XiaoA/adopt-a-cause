<%= render 'shared/content_title', title: "Available Projects" %>

<div class="container-fluid" id="project-well">
  <% @project.each do |project| %>
    <ul class="project-list">
      <li class="well col-md-4 col-md-offset-1 text-center">
<%= project.project_name %> <br> <%= link_to 'Volunteer', new_project_user_path(:project_id => project.id, :user_id => current_user.id), :method => :post, :action => :create, class: "btn btn-primary" %> <%= link_to "More Info", project_path(project), class: "btn btn-warning" %>
      </li>
    </ul>
  <% end %>
</div>

