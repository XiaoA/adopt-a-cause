<%= render 'shared/content_title', title: "#{@user.username}'s Projects" %>

<div class="container-fluid">
  <div class="row">
    <div class="col-md-4 col-md-offset-1">
      <% @user.projects.each do |project| %>
				<ul>
					<li class="project-list"><%= project.project_name %> <%= link_to "Info", project_path, class: "btn btn-info" %></li>
				</ul>
      <% end %>
    </div>
  </div>
</div>
